<!DOCTYPE html>
<html lang="fr">
	<head>
		<meta charset="UTF-8">
		<title>Mon Application</title>
		<link rel="stylesheet" type="text/css" href="css/style.css">
	</head>
	<body>
		<table class="tableau">
		<% let noLigne = 0 %>
			<% for (elm of adresses) { %>
				<% if (noLigne == 0){ %>
				<tr>
					<% noLigne++ %>
					<% for (p in elm) {%>
						<th><%= p %></th>
					<% } %>
				<% } %>
				<tr>

					<% for (p in elm) {%>
					<td><%= elm[p] %></td>
					<% } %>

<!-- 				<td><%= elm.prenom %></td>
				<td><%= elm.nom %></td>
				<td><%= elm.telephone %></td> -->
				</tr>
			<% } %>
		</table>
	</body>
</html>